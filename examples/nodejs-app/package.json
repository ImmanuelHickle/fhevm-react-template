{
  "name": "nodejs-fhevm-example",
  "version": "1.0.0",
  "description": "Node.js CLI example using @fhevm/sdk",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "fhevm-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "encrypt": "tsx src/index.ts encrypt",
    "decrypt": "tsx src/index.ts decrypt",
    "info": "tsx src/index.ts info"
  },
  "dependencies": {
    "@fhevm/sdk": "workspace:*",
    "ethers": "^6.10.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  }
}
